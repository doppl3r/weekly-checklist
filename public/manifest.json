{
  "manifest_version": 3,
  "name": "Weekly Checklist",
  "description": "Weekly checklist application",
  "version": "1.0.2",
  "action": {
    "default_popup": "index.html",
    "default_title": "Weekly Checklist"
  },
  "icons": { 
    "16": "png/icon16.png", 
    "32": "png/icon32.png",
    "48": "png/icon48.png", 
    "128": "png/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}